{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
		
	"pages": [
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "",
				"navigationStyle":"custom",
				"backgroundColorBottom": "#252525",
				"backgroundColor":"#252525",
				"navigationBarTextStyle": "black"
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarBackgroundColor": "#252525",
				"navigationBarTitleText": "",
				"navigationStyle":"custom",
				"backgroundColorBottom": "#252525",
				"backgroundColor":"#252525"
			}
		},
		{
			"path": "pages/personInfo/personInfo",
			"style": {
				"navigationBarTitleText": "",
				"navigationBarBackgroundColor": "#252525",
				"navigationBarTextStyle": "white",
				"backgroundColorBottom": "#222222",
				"backgroundColor":"#252525",
				"navigationStyle":"custom"
			}
		}
	],
	"subPackages": [
		{
			"root": "digitalCollectPage",
			"pages": [
				{
					"path": "pages/digitalIndex/index/index",
					"style": {
						"navigationBarTitleText": "藏品收集",
						"navigationBarBackgroundColor": "#ffffff",
						"backgroundColorTop": "#fff,",
						"backgroundColorBottom": "#fff"
					}
				},
				{
					"path": "pages/editPersonData/index/index",
					"style": {
						"navigationStyle":"custom",
						"navigationBarBackgroundColor": "#252525",
						"navigationBarTextStyle": "white",
						"backgroundColorBottom": "#222222",
						"backgroundColor":"#252525"
					}
				},
				{
					"path": "pages/set/index/index",
					"style": {
						"navigationStyle":"custom",
						"navigationBarBackgroundColor": "#252525",
						"navigationBarTextStyle": "white",
						"backgroundColorBottom": "#222222",
						"backgroundColor":"#252525"
					}
				}
			]
		}
	],
	"preloadRule": {
		"digitalCollectPage/pages/digitalIndex/index/index": {
				"network": "all",
				"packages": ["digitalCollectPage"]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar":{
	"color":"#FFFFFF",
	"selectedColor":"#aaaa00",
	"backgroundColor":"#000000",
	"height": "80px",
	"borderStyle":"white",
	"list":[
			{
				"pagePath":"pages/index/index",
				"text":"首页",
				"iconPath":"static/img/index.png",
				"selectedIconPath":"static/img/index-current.png"
			},
			{
				"pagePath":"pages/personInfo/personInfo",
				"text":"我的",
				"iconPath":"static/img/my.png",
				"selectedIconPath":"static/img/my-current.png"
			}
		]
	}
}
